    "standard",
    "code",
    "style"
  ],
  "author": "Krzysztof Kaczor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krzkaczor/standardts/issues"
  },
  "homepage": "https://github.com/krzkaczor/standardts#readme",
  "dependencies": {
    "commander": "2.9.0",
    "glob": "7.0.3",
    "lodash": "4.11.1",
    "tslint": "3.7.4",
    "tslint-eslint-rules": "1.2.0",
    "typescript": "1.8.10"
  },
  "devDependencies": {
    "typescript": "1.8.10"
  }
