{
  "semicolon": [
    true,
    "never"
  ],
  "no-unexpected-multiline": true,
  "comment-format": [
    true,
    "check-space"
  ],
  "eofline": true,
  "quotemark": [
    true,
    "single",
    "avoid-escape"
  ],
  "no-unused-variable": [
    true,
    "react"
  ],
  "triple-equals": [
    true,
    "allow-null-check"
  ],
  "whitespace": [
    true,
    "check-branch",
    "check-operator"
  ]
}
